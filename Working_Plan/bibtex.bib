@article{Andres07,
    author={A. De Virgiliis and R. L. C. Vink and J. Horbach and K. Binder},
      title={Colloid-polymer mixtures between asymmetric walls: Evidence for an interface localization transition},
        journal={EPL (Europhysics Letters)},
	  volume={77},
	    number={6},
	      pages={60002},
	        Murl={http://stacks.iop.org/0295-5075/77/i=6/a=60002},
		  year={2007},
		    abstract={We demonstrate via computer simulation that mixtures of colloids and polymers confined to thin films have the ability to undergo an interface localization transition. While one wall of the film is assumed to be hard for both particles, at the other wall, an additional repulsive potential acts, but on the colloids only. by varying the strength of this repulsion, a crossover from capillary condensation to interface localization is found. the latter occurs under conditions where in the bulk almost complete phase separation has occurred.}
}


@Article{Egorov_15,
  Title                    = {Semiflexible polymer brushes and the brush-mushroom crossover},
  Author                   = {Egorov, Sergei A. and Hsu, Hsiao-Ping and Milchev, Andrey and Binder, Kurt},
  Journal                  = {Soft Matter},
  Year                     = {2015},
  Pages                    = {2604-2616},
  Volume                   = {11},

  Abstract                 = {Semiflexible polymers end-grafted to a repulsive planar substrate under good solvent conditions are studied by scaling arguments{,} computer simulations{,} and self-consistent field theory. Varying the chain length N{,} persistence length lp{,} and grafting density [sigma]g{,} the chain linear dimensions and distribution functions of all monomers and of the free chain ends are studied. Particular attention is paid to the limit of very small [sigma]g{,} where the grafted chains behave as {"}mushrooms{"} no longer interacting with each other. Unlike a flexible mushroom{,} which has a self-similar structure from the size (a) of an effective monomer up to the mushroom height (h/a [is proportional to] Nv{,} [small nu] [approximate] 3/5){,} a semiflexible mushroom (like a free semiflexible chain) exhibits three different scaling regimes{,} h/a [is proportional to] N for contour length L = Na < lp{,} a Gaussian regime{,} h/a [is proportional to] (Llp)1/2/a for lp [double less-than] L [double less-than] R* [is proportional to] (lp2/a){,} and a regime controlled by excluded volume{,} h/a [is proportional to] (lp/a)1/5N[small nu]. The semiflexible brush is predicted to scale as h/a [is proportional to] (lpa[sigma]g)1/3N in the excluded volume regime{,} and h/a [is proportional to] (lpa3[sigma]2)1/4N in the Gaussian regime. Since in the volume taken by a semiflexible mushroom excluded-volume interactions are much weaker in comparison to a flexible mushroom{,} there occurs an additional regime where semiflexible mushrooms overlap without significant chain stretching. Moreover{,} since the size of a semiflexible mushroom is much larger than the size of a flexible mushroom with the same N{,} the crossover from mushroom to brush behavior is predicted to take place at much smaller densities than for fully flexible chains. The numerical results{,} however{,} confirm the scaling predictions only qualitatively; for chain lengths that are relevant for experiments{,} often intermediate effective exponents are observed due to extended crossovers.},
  Doi                      = {10.1039/C4SM02862G},
  File                     = {Egorov_15.pdf:brushes/Egorov_15.pdf:PDF},
  Issue                    = {13},
  Owner                    = {pastorin},
  Publisher                = {The Royal Society of Chemistry},
  Timestamp                = {2015.03.31},
  Url                      = {http://dx.doi.org/10.1039/C4SM02862G}
}

